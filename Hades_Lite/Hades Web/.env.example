# App
APP_NAME=Hades API
APP_VERSION=1.0.0
DEBUG=true

# Database
DATABASE_URL=postgresql://hades:hades_dev@localhost:5432/hades_db

# Redis
REDIS_URL=redis://localhost:6379/0

# Keycloak (usar el mismo cliente que Hades Ultimate)
KEYCLOAK_SERVER_URL=https://your-keycloak-server.com
KEYCLOAK_REALM=your-realm
KEYCLOAK_CLIENT_ID=hades-web
KEYCLOAK_CLIENT_SECRET=your-client-secret

# Gemini
GEMINI_API_KEY=your-gemini-api-key

# CORS (separados por comas)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Upload
MAX_UPLOAD_SIZE=10485760

# Async Processing (Toggle)
ASYNC_PROCESSING=false
